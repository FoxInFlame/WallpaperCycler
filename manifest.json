{
  "manifest_version": 2,
  "name": "WallpaperCycler",
  "author": "FoxInFlame",
  "description": "Cycle through Wallpapers that you set!",
  "version": "1.1",
  "offline_enabled": false,

  "permissions": [
    "storage",
    "wallpaper",
    "alarms",
    "background"
  ],
  "background": {
    "scripts": [
      "jquery-2.2.3.min.js",
      "x2js.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "options_page": "options.html",
  
  "icons": {
    "128": "icon.png"
  }
}